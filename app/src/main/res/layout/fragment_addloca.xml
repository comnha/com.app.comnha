<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:background="@android:color/white"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    android:weightSum="5"
    tools:context="com.app.ptt.comnha.Fragment.AddstoreFragment">

    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="2dp">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar_addloca"
            style="@style/Base.ThemeOverlay.AppCompat.Dark.ActionBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </android.support.design.widget.AppBarLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="16dp"
        android:layout_weight="4.5"
        android:orientation="vertical">

        <android.support.design.widget.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/frg_addloction_edt_storename"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@android:color/transparent"
                android:drawableStart="@drawable/ic_store_50black_24dp"
                android:gravity="start|center_vertical"
                android:hint="@string/text_tenquan"
                android:inputType="text"
                android:paddingEnd="4dp"
                android:paddingStart="4dp"
                android:textColor="@android:color/black" />
        </android.support.design.widget.TextInputLayout>

        <android.support.design.widget.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/frg_addloction_edt_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@android:color/transparent"
                android:drawableStart="@drawable/ic_location_on_black_24dp"
                android:gravity="start|center_vertical"
                android:hint="@string/text_address"
                android:inputType="textPostalAddress"
                android:paddingEnd="4dp"
                android:paddingStart="4dp"
                android:textColor="@android:color/black" />
        </android.support.design.widget.TextInputLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="7">

            <ImageView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:paddingStart="4dp"
                android:src="@drawable/ic__time_50black_24dp" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:gravity="center_vertical"
                android:padding="4dp"
                android:text="@string/text_openningTime" />

            <Button
                android:id="@+id/frg_addloction_btn_opentime"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.25"
                android:background="@android:color/transparent"
                android:textColor="@color/addlocal_color_opentime" />

            <Button
                android:id="@+id/frg_addloction_btn_closetime"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.25"
                android:background="@android:color/transparent"
                android:textColor="@color/addlocal_color_opentime" />
        </LinearLayout>

        <android.support.design.widget.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/frg_addloction_edt_phonenumb"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@android:color/transparent"
                android:drawableStart="@drawable/ic_call_50black_24dp"
                android:gravity="start|center_vertical"
                android:hint="@string/text_sdt"
                android:inputType="phone"
                android:paddingEnd="4dp"
                android:paddingStart="4dp"
                android:textColor="@android:color/black" />
        </android.support.design.widget.TextInputLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/frg_addloction_txtv_avatar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Ảnh đại diện" />

            <ImageView
                android:id="@+id/frg_addloction_imgv_avatar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="fitXY" />

        </RelativeLayout>


    </LinearLayout>


    <TextView
        android:id="@+id/frg_addloction_txtv_save"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_gravity="bottom"
        android:layout_weight="0.5"
        android:background="@color/color_selection_report"
        android:clickable="true"
        android:foreground="?attr/selectableItemBackground"
        android:gravity="center"
        android:text="@string/text_saveaddloca"
        android:textAllCaps="true"
        android:textColor="@android:color/white"
        android:textStyle="bold" />
